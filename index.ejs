<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <title><%= htmlWebpackPlugin.options.title%></title>
    <% htmlWebpackPlugin.files.css.forEach(function(item){ %>
        <link rel="stylesheet" type="text/css" href="<%= item %>">
    <% }) %>
</head>
<body id="app" class="pace-done">
    <div id="app"><router-view ></router-view></div>
    <script>
        if(window.location.href.indexOf('finance.kashuo.net')<0){
            window.origin = 'https://fin.qa.kashuo.net/v1/finance';
            window.authUrl1 = 'http://auth2.qa.kashuo.net';
            window.authUrl2 = 'https://fin.qa.kashuo.net';
        }else{
            window.origin = 'https://finance.kashuo.net/v1/finance';
            window.authUrl1 = 'http://passport.kashuo.net';
            window.authUrl2 = 'https://finance.kashuo.net';
        }
    </script>
    <script type="text/javascript" src="./jquery.js"></script>
    <%  ['bootstrap','vuecore','app'].map(function(item){
            return htmlWebpackPlugin.files.js.map(function(real){
                if(~real.indexOf(item)) return real
            }).join('')
        }).forEach(function(item){ %>
        <script type="text/javascript" src="<%= item %>"></script>
    <% }) %>
    <script src="./dist/authKit.js"></script>
</body>
</html>